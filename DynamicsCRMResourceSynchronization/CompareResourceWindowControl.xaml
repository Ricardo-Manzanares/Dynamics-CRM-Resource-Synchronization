<UserControl x:Class="DynamicsCRMResourceSynchronization.CompareResourceWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:CustomControls="clr-namespace:DynamicsCRMResourceSynchronization.Controls"
             xmlns:Converters="clr-namespace:DynamicsCRMResourceSynchronization.Converters"
             mc:Ignorable="d"
             Name="CompareResource" SizeChanged="CompareResource_SizeChanged">
    <UserControl.Resources>
        <Converters:NameTypeResourceConverter x:Key="NameTypeResourceConverter" />
        <Converters:NameDifferenceResourceConverter x:Key="NameDifferenceResourceConverter" />
        <Converters:StatusRowDifferenceResourceConverter x:Key="StatusRowDifferenceResourceConverter" />
    </UserControl.Resources>
    <Grid Background="{DynamicResource {x:Static SystemColors.MenuBrushKey}}">
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <CustomControls:LoadingControl Visibility="Hidden" x:Name="Loading"/>           
                <TextBlock x:Name="LoadingText" Text="" Padding="0, 10, 0, 0" Background="Transparent" HorizontalAlignment="Center"></TextBlock>
            </Grid>            
        </StackPanel>
        <StackPanel Grid.Row="1">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock Text="Environmental solutions" Margin="10, 0, 10, 0" ></TextBlock>
            </Grid>
        </StackPanel>
        <StackPanel Grid.Row="2">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="50" />
                </Grid.ColumnDefinitions>
                <ComboBox x:Name="CRMSolutions" Grid.Column="0" Margin="10" IsEnabled="False" SelectionChanged="CRMSolutions_SelectionChanged" />
                <Button Grid.Column="1" Click="CRMLoadSolutions_Click" Background="Transparent" ToolTip="Load environment solutions" x:Name="CRMLoadSolutions" BorderBrush="Transparent" Height="30" Width="30" BorderThickness="0" Cursor="Hand" >
                    <Image>
                        <Image.Source>
                            <DrawingImage>
                                <DrawingImage.Drawing>
                                    <DrawingGroup ClipGeometry="M0,0 V56.32 H64 V0 H0 Z">
                                        <DrawingGroup Opacity="1">
                                            <DrawingGroup Opacity="1">
                                                <GeometryDrawing Brush="#FF35353D" Geometry="F1 M64,56.32z M0,0z M63.22,22.34A2.57,2.57,0,0,0,59.6,22.4L56.21,25.92A28.13,28.13,0,1,0,48.27,47.87A2.56,2.56,0,0,0,44.61,44.29A23,23,0,1,1,51.08,25.84L47.89,22.65A2.56,2.56,0,1,0,44.27,26.27L52,34A2.56,2.56,0,0,0,53.81,34.75L53.81,34.75A2.56,2.56,0,0,0,55.63,33.96L63.28,26A2.57,2.57,0,0,0,63.22,22.34z" />
                                            </DrawingGroup>
                                        </DrawingGroup>
                                    </DrawingGroup>
                                </DrawingImage.Drawing>
                            </DrawingImage>
                        </Image.Source>
                    </Image>
                </Button>
                <Border />
            </Grid>
        </StackPanel>
        <StackPanel Grid.Row="3">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock Text="Solution resources" Margin="10, 10, 10, 0" ></TextBlock>
            </Grid>
        </StackPanel>
        <StackPanel Grid.Row="4">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="50" />
                </Grid.ColumnDefinitions>
                <TextBox Grid.Column="0" x:Name="CRMNameSearchResource" TextWrapping="Wrap" IsEnabled="False" Margin="10" GotFocus="TextBox_GotFocus" BorderBrush="Transparent" LostFocus="TextBox_LostFocus" />
                <ComboBox Grid.Column="1" x:Name="CRMTypeResource" IsEnabled="False" Margin="10">
                    <ComboBoxItem Content="-- Type or resource --" Tag="0" />
                    <ComboBoxItem Content="Web page (HTML) .htm, .html" Tag="1" />
                    <ComboBoxItem Content="Stylesheet (CSS) .css" Tag="2" />
                    <ComboBoxItem Content="Script (JScript) .js" Tag="3" />
                    <ComboBoxItem Content="Data (XML) .xml" Tag="4" />
                    <ComboBoxItem Content="Image (PNG) .png" Tag="5" />
                    <ComboBoxItem Content="Image (JPG) .jpg" Tag="6" />
                    <ComboBoxItem Content="Image (GIF) .gif" Tag="7" />
                    <ComboBoxItem Content="Silverlight (XAP) .xap" Tag="8" />
                    <ComboBoxItem Content="Stylesheet (XSL) .xsl, xslt" Tag="9" />
                    <ComboBoxItem Content="Image (ICO) .ico" Tag="10" />
                    <ComboBoxItem Content="Vector format (SVG) .svg" Tag="11" />
                    <ComboBoxItem Content="Chain (RESX) .resx" Tag="12" />
                </ComboBox>
